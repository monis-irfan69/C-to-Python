// C++ Simple Sort //

#include <bits/stdc++.h>
using namespace std;
void sortit(int arr[], int n)
{
	for (int i = 0; i < n; i++) {
	arr[i]=i+1;
	}
}
int main()
{
	int arr[] = { 10, 7, 9, 2, 8, 3, 5, 4, 6, 1 };
	int n = sizeof(arr) / sizeof(arr[0]);
	sortit(arr, n);
	for (int i = 0; i < n; i++)
		cout << arr[i] << " ";
}


//  End //



// Python Code //

using System;
using System.Collections.Generic;
class GFG {
	static void sortit(int []arr, int n)
	{
		for (int i = 0; i < n; i++)
		{
	
			arr[i]=i+1;
		}
	}
		public static void Main()
	{
		int []arr = {10, 7, 9, 2, 8,
					3, 5, 4, 6, 1};
		int n = arr.Length;
			sortit(arr, n);
		for (int i = 0; i < n; i++)
			Console.Write(arr[i] + " ");
	}
}
